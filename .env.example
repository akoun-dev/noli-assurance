# Configuration Supabase - Remplacer par vos propres valeurs
NEXT_PUBLIC_SUPABASE_URL="https://your-project.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-anon-key-here"
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key-here"

# NextAuth.js
NEXTAUTH_SECRET="your-nextauth-secret-here"
NEXTAUTH_URL="http://localhost:3000"

# Configuration CORS (s√©parer les origines par des virgules)
ALLOWED_ORIGINS="http://localhost:3000,https://your-domain.com"

# Configuration Redis (optionnel, pour le cache)
REDIS_URL="redis://localhost:6379"

# Configuration Sentry pour le monitoring des erreurs
SENTRY_DSN="https://your-sentry-dsn-here"
SENTRY_ORG="your-sentry-organization"
SENTRY_PROJECT="your-sentry-project"
SENTRY_TRACES_SAMPLE_RATE="0.1"
SENTRY_PROFILES_SAMPLE_RATE="0.1"
SENTRY_ENVIRONMENT="development"
