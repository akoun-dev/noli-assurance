# Configuration Netlify
NETLIFY_SITE_ID=your-site-id
NETLIFY_AUTH_TOKEN=your-auth-token
NETLIFY_DOMAIN=noliassurance.netlify.app

# Variables d'environnement Next.js
NEXT_PUBLIC_SITE_URL=https://noliassurance.netlify.app
NEXT_PUBLIC_VERCEL_URL=https://noliassurance.netlify.app

# Configuration de build
NEXT_PUBLIC_IS_NETLIFY=true
NETLIFY_USE_YARN=true

# Variables optionnelles
NETLIFY_BUILD_COMMAND="yarn build"
NETLIFY_PUBLISH_DIR="out"